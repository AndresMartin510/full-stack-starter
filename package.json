{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "webpack-dev": "cross-env NODE_ENV=development webpack --mode development",
    "webpack-prod": "cross-env NODE_ENV=production webpack --mode production",
    "build": "npm run build:prod",
    "build:dev": "webpack-dev-server",
    "build:prod": "npm run build:clean && ngc && npm run webpack-prod && npm run build:clean",
    "build:clean": "del-cli 'client/**/*.js' 'client/**/*.js.map' 'client/**/*.ngsummary.json' 'client/**/*.metadata.json' 'client/**/**/*.ngfactory.ts' 'client/**/*.ngstyle.ts' 'client/**/*.shim.ts'",
    "start": "nodemon -L -V --ignore ./client ./bin/www"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "aws-sdk": "^2.817.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bootstrap": "~4.5.3",
    "cleave.js": "^1.6.0",
    "connect-flash": "^0.1.1",
    "cookie-session": "^1.3.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "ejs": "~2.5.7",
    "email-templates": "^5.1.0",
    "express": "~4.16.0",
    "express-ejs-layouts": "^2.5.0",
    "express-fileupload": "^1.2.0",
    "fs-extra": "^8.1.0",
    "http-errors": "~1.6.2",
    "i18n": "^0.8.6",
    "inflection": "^1.12.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "mime-types": "^2.1.27",
    "mkdirp": "^0.5.1",
    "morgan": "~1.9.0",
    "mv": "^2.1.1",
    "nodemon": "^2.0.6",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^7.18.2",
    "popper.js": "^1.16.0",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-paginate": "^1.1.6",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "cross-env": "~5.2",
    "del-cli": "~1.1",
    "foreman": "^3.0.1",
    "node-sass": "^4.13.1"
  }
}
